{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "webindex": {
            "matchCondition": {
                "route": "/"
            },
            "backendUri": "https://YOURUNIQUEWORDstor.blob.core.windows.net/web/index.html"
        },
        "web": {
            "matchCondition": {
                "route": "/{*url}"
            },
            "backendUri": "https://YOURUNIQUEWORDstor.blob.core.windows.net/web/{url}"
        },
        "Dashboard": {
            "matchCondition": {
                "route": "/dashboard"
            },
            "backendUri": "https://YOURUNIQUEWORDstor.blob.core.windows.net/web/index.html"
        }
    }
}